<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
	    xmlns:tools="http://schemas.android.com/tools"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	    android:background="#8c2121">

	<RelativeLayout 
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:paddingBottom="@dimen/activity_vertical_margin"
	    android:paddingLeft="@dimen/activity_horizontal_margin"
	    android:paddingRight="@dimen/activity_horizontal_margin"
	    android:paddingTop="@dimen/activity_vertical_margin"
	    tools:context=".StudentLoginActivity"
	    android:background="#8c2121" >
	
	    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="Prospective Student Information"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textColor="#999999" />
	    
	    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView1"
        android:text="Name *"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#999999" />
	    
	    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView2"
        android:text="Date of Birth"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#999999" />
	    
	    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView3"
        android:text="Gender"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#999999" />
	    
	    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView4"
        android:text="Year in School *"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#999999" />
	    
	    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView5"
        android:text="GPA *"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#999999" />
	    
	    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView6"
        android:text="Did you take SAT/ACT? *"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#999999" />
	    
	    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView7"
        android:text="High School Name"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#999999" />
	    
	    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView8"
        android:text="High School City"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#999999" />
	    
	    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView9"
        android:text="High School State"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#999999" />
	    
	    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView10"
        android:text="Contact Information"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textColor="#999999" />
	    
	    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView11"
        android:text="Name *"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#999999" />
	    
	    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView12"
        android:text="Relationship to Student"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#999999" />
	    
	    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView13"
        android:text="E-mail Address *"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#999999" />
	    
	    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView14"
        android:text="Home Phone"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#999999" />
	    
	    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView15"
        android:text="Cell Phone"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#999999" />
	    
	    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView16"
        android:text="Address *"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#999999" />
	    
	    <TextView
        android:id="@+id/textView18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView17"
        android:text="Address Line 1 *"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#999999" />
	    
	    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView18"
        android:text="Address Line 2"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#999999" />
	    
	    <TextView
        android:id="@+id/textView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView19"
        android:text="City *"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#999999" />
	    
	    <TextView
        android:id="@+id/textView21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView20"
        android:text="State / Province / Region *"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#999999" />
	    
	    <TextView
        android:id="@+id/textView22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView21"
        android:text="Postal / Zip Code * "
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#999999" />
	    
	    <TextView
        android:id="@+id/textView23"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView22"
        android:text="Country *"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#999999" />
	    
	    <TextView
        android:id="@+id/textView24"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView23"
        android:text="About the Visit"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textColor="#999999" />
	    
	    <TextView
        android:id="@+id/textView25"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView24"
        android:text="Date of Visit (MM/DD/YYYY) *"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#999999" />
	    
	    <TextView
        android:id="@+id/textView26"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView25"
        android:text="How many in your party? *"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#999999" />
	    
	    <TextView
        android:id="@+id/textView27"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView26"
        android:text="Time you need to start (HH:MM am/pm)"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#999999" />
	    
	    <TextView
        android:id="@+id/textView28"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView27"
        android:text="Time you need to be finished (HH:MM am/pm)"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#999999" />
	
	</RelativeLayout>

</ScrollView>